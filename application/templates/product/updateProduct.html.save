{% extends "layout.html" %}
{% extends "layout.html" %}
{% block body %}
<p>If there are no categories or the needed category is not yet created, please create some first.</p>
<form method="POST" action="{{ url_for('product_create') }}">
  <table>
    <tr>
      <td>
        {{ form.name.label }}
      </td>
      <td>
        {{ form.name }}
      </td>
      <td>
        <ul>
          {% for error in form.name.errors %}
          <li>{{ error }}</li>
          {% endfor %}
        </ul>
      </td>
    </tr>
    <tr>
      <td>
       
      <td>
        {{ form.price }}
      </td>
      <td>
        <ul>
          {% for error in form.price.errors %}
          <li>{{ error }}</li>
          {% endfor %}
        </ul>
      </td>
    </tr>
    <tr>
      <td colspan="6">
        <input type="submit" value="Update"/>
      </td>
    </tr>
  </form>
</table>
{% endblock %}



